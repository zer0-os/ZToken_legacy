{
  "compilerOptions": {
    "target": "es2018",
    "lib": ["ES2018", "DOM", "ES2018.PROMISE"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "resolveJsonModule": true,
    "outDir": "typechain-js"
  },
  "include": ["./typechain"]
}
